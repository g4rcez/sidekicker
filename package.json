{
    "name": "sidekicker",
    "version": "0.0.10",
    "description": "An abstract lib to help in our JS/TS projects",
    "main": "lib/index.js",
    "scripts": {
        "test": "jest",
        "watch-node": "nodemon lib/index.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-ts": "tsc -w",
        "build": "tsc -b",
        "lint": "tslint --config tslint.json -p .",
        "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
        "start": "npm run watch"
    },
    "author": "vandalvnl",
    "repository": {
        "type": "github",
        "url": "https://github.com/vandalvnl/sidekicker"
    },
    "license": "MIT",
    "dependencies": {
        "@types/ramda": "^0.25.46",
        "axios": "^0.18.0",
        "moment": "^2.22.2",
        "query-string": "^6.2.0",
        "ramda": "^0.25.0"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.24",
        "@types/jest": "^23.3.10",
        "@types/node": "^10.12.21",
        "babel-core": "^6.26.3",
        "babel-jest": "^24.1.0",
        "concurrently": "^4.0.1",
        "jest": "^24.1.0",
        "nock": "^10.0.2",
        "nodemon": "^1.18.6",
        "prettier": "^1.16.4",
        "regenerator-runtime": "^0.13.1",
        "ts-jest": "^23.10.5",
        "typescript": "^3.1.6"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/__utils"
        ]
    }
}
