{
  "name": "sidekicker",
  "version": "0.0.1",
  "description": "Lots of typescript utility functions",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "format": "prettier --write './{src,tests}/**/*.ts'",
    "rm": "rm -rf build",
    "rm:config": "rm -rf build",
    "build": "tsc -p tsconfig.json && rm -rf tsconfig.tsbuildinfo",
    "compile": "npm-run-all -s rm build rm:config",
    "test": "vitest run",
    "test:browser": "vitest --environment happy-dom run"
  },
  "author": "Allan Garcez <allan.f.garcez@gmail.com> (https://github.com/g4rcez)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.12.7",
    "@types/qs": "6.9.15",
    "happy-dom": "^14.7.1",
    "prettier": "2.8.8",
    "ts-toolbelt": "9.6.0",
    "typescript": "5.4.5",
    "vite": "5.2.9",
    "vitest": "1.5.0"
  },
  "volta": {
    "node": "20.12.2",
    "yarn": "1.22.19",
    "npm": "10.5.2"
  },
  "dependencies": {
    "qs": "6.11.2"
  }
}
