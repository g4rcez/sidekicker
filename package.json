{
  "name": "sidekicker",
  "version": "0.1.2",
  "description": "Lots of typescript utility functions",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.ts",
        "default": "./dist/index.d.ts"
      },
      "default": {
        "type": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "format": "prettier --write './{src,tests}/**/*.ts'",
    "rm": "rm -rf build",
    "rm:config": "rm -rf build",
    "build": "tsup",
    "compile": "npm-run-all -s rm build rm:config",
    "test": "vitest run",
    "test:browser": "vitest --environment happy-dom run"
  },
  "author": "Allan Garcez <allan.f.garcez@gmail.com> (https://github.com/g4rcez)",
  "license": "MIT",
  "dependencies": {
    "qs": "6.11.2"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@types/qs": "6.9.15",
    "happy-dom": "^14.7.1",
    "prettier": "2.8.8",
    "ts-toolbelt": "9.6.0",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "vite": "5.2.9",
    "vitest": "1.5.0"
  },
  "volta": {
    "node": "20.12.2",
    "yarn": "1.22.19",
    "npm": "10.5.2"
  }
}
